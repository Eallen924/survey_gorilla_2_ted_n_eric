<div class="container">
  <h1><%= @survey.title %></h1>
  <div class="questions">
    <ul>
      <% @survey.questions.each do |question|%>
      <li><%= question.content %></li>
      <ul><% question.choices.each do |choice|  %>
        <li><%= choice.content %></li>
        <% end %>
      </ul>
      <% end %>
    </ul>
  </div>
</div>
